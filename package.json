{
  "name": "@cardbrother/nestjs-tencent-cloud-sdk",
  "version": "1.0.1",
  "main": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "description": "nestjs tencent cloud sdk",
  "author": "guotingchaopr@gmail.com",
  "license": "MIT",
  "bin": "dist/index.js",
  "organization": "cardbrother",
  "repository": {
    "type": "github",
    "url": "https://github.com/guotingchao/nest-tencent-cloud-sdk"
  },
  "scripts": {
    "build": "nest build",
    "changeset": "changeset",
    "publish:release": "changeset publish",
    "publish:version": "changeset version",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://opensource.org/licenses/MIT"
    }
  ],
  "keywords": [
    "Nest",
    "NestJS",
    "TencentCloud",
    "TencentCloud-sdk",
    "Nest-TencentCloud",
    "Nest-TencentCloud-SDK",
    "@cardbrother"
  ],
  "dependencies": {
    "@nestjs/common": "^10.3.7",
    "cos-nodejs-sdk-v5": "^2.13.4",
    "tencentcloud-sdk-nodejs": "^4.0.837"
  },
  "devDependencies": {
    "@changesets/action": "github:changesets/action",
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.1",
    "@nestjs/cli": "^10.3.2",
    "@nestjs/config": "^3.2.2",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.7",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.7",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "eslint-plugin-unused-imports": "^3.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "passWithNoTests": true
  },
  "publishConfig": {
    "access": "public",
    "scope": "@cardbrother",
    "tag": "latest",
    "registry": "https://registry.npmjs.org/"
  },
  "bugs": {
    "url": "https://github.com/guotingchao/nest-tencent-cloud-sdk/issues",
    "email": "guotingchaopr@gmail.com"
  },
  "homepage": "https://github.com/guotingchao/nest-tencent-cloud-sdk",
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md",
    "test"
  ]
}
